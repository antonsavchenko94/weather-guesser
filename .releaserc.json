{
  "branches": [
    "master",
    {
      "name": "stage",
      "prerelease": true
    },
    {
      "name": "edge",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "docs/CHANGELOG.md"
      }
    ],
    "@semantic-release/npm"
  ]
}